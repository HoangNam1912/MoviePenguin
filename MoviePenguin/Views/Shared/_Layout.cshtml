
<!DOCTYPE html>
<html lang="en">
<head>
    <title>PenguinMovie</title>
 
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="One Movies Responsive web template, Bootstrap Web Templates, Flat Web Templates, Android Compatible web template,
Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />
    <script type="application/x-javascript">
        addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false);
               function hideURLbar(){ window.scrollTo(0,1); } </script>
    <link href="~/assets/web/css/bootstrap.css" rel="stylesheet" type="text/css" media="all" />
    <link href="~/assets/web/css/style.css" rel="stylesheet" type="text/css" media="all" />
    <link rel="stylesheet" href="~/assets/web/css/contactstyle.css" type="text/css" media="all" />
    <link rel="stylesheet" href="~/assets/web/css/faqstyle.css" type="text/css" media="all" />
    <link href="~/assets/web/css/single.css" rel='stylesheet' type='text/css' />
    <link href="~/assets/web/css/medile.css" rel='stylesheet' type='text/css' />
    <!-- news-css -->
    <link rel="stylesheet" href="~/assets/web/news-css/news.css" type="text/css" media="all" />
    <!-- //news-css -->
    <!-- list-css -->
    <link rel="stylesheet" href="~/assets/web/list-css/list.css" type="text/css" media="all" />
    <!-- //list-css -->
    <!-- banner-slider -->
    <link href="~/assets/web/css/jquery.slidey.min.css" rel="stylesheet">
    <!-- //banner-slider -->
    <!-- pop-up -->
    <link href="~/assets/web/css/popuo-box.css" rel="stylesheet" type="text/css" media="all" />
    <!-- //pop-up -->
    <!-- font-awesome icons -->
    <link rel="stylesheet" href="~/assets/web/css/font-awesome.min.css" />
    <!-- //font-awesome icons -->
    <!-- js -->
    <script type="text/javascript" src="~/assets/web/js/jquery-2.1.4.min.js"></script>
    <!--Live chat-->
    <link href="~/assets/web/css/livechat.css" rel="stylesheet" />
    <script src="https://connect.facebook.net/en_US/sdk.js#xfbml=1&version=v2.9"></script>
    <script>jQuery(document).ready(function ($) { function detectmob() { if (navigator.userAgent.match(/Android/i) || navigator.userAgent.match(/webOS/i) || navigator.userAgent.match(/iPhone/i) || navigator.userAgent.match(/iPad/i) || navigator.userAgent.match(/iPod/i) || navigator.userAgent.match(/BlackBerry/i) || navigator.userAgent.match(/Windows Phone/i)) { return true; } else { return false; } } var t = { delay: 125, overlay: $(".fb-overlay"), widget: $(".fb-widget"), button: $(".fb-button") }; setTimeout(function () { $("div.fb-livechat").fadeIn() }, 8 * t.delay); if (!detectmob()) { $(".ctrlq").on("click", function (e) { e.preventDefault(), t.overlay.is(":visible") ? (t.overlay.fadeOut(t.delay), t.widget.stop().animate({ bottom: 0, opacity: 0 }, 2 * t.delay, function () { $(this).hide("slow"), t.button.show() })) : t.button.fadeOut("medium", function () { t.widget.stop().show().animate({ bottom: "30px", opacity: 1 }, 2 * t.delay), t.overlay.fadeIn(t.delay) }) }) } });</script>
    <!-- //js -->
    <!--Dark Mode-->
    <script src="~/assets/web/js/JavaScript.js"></script>
    <!-- banner-bottom-plugin -->
    <link href="~/assets/web/css/owl.carousel.css" rel="stylesheet" type="text/css" media="all">
    <script src="~/assets/web/js/owl.carousel.js"></script>
    <script>
        $(document).ready(function () {
            $("#owl-demo").owlCarousel({
                autoPlay: 5000, //Set AutoPlay to 3 seconds
                items: 5,
                itemsDesktop: [640, 4],
                itemsDesktopSmall: [414, 3]

            });

        });
    </script>
    <!-- //banner-bottom-plugin -->
    <link href='//fonts.googleapis.com/css?family=Roboto+Condensed:400,700italic,700,400italic,300italic,300' rel='stylesheet' type='text/css'>
    <!-- start-smoth-scrolling -->
    <script type="text/javascript" src="~/assets/web/js/move-top.js"></script>
    <script type="text/javascript" src="~/assets/web/js/easing.js"></script>
    <script type="text/javascript">
        jQuery(document).ready(function ($) {
            $(".scroll").click(function (event) {
                event.preventDefault();
                $('html,body').animate({ scrollTop: $(this.hash).offset().top }, 1000);
            });
        });
    </script>

</head>

<body>
    <!-- header -->
    <div class="header">
        <div class="container">
            <div class="w3layouts_logo">
                <!-- logo -->
                <a href="/Home/Index">
                    <h1>Penguin<span>Movie</span></h1> 
                </a>             
            </div>
            <div class="w3_search">
                <form name="timkiem" action="/Home/Search">
                    <input type="text" name="searchKey" placeholder="Nhập tên phim">
                    <input type="submit" name="submit" value="Tìm kiếm">
                </form>
            </div>
            @Html.Partial("_LoginPartial")       
        </div>
    </div>
    <!-- //header -->
    <!-- bootstrap-pop-up -->
    <script>
        $('.toggle').click(function () {
            // Switches the Icon
            $(this).children('i').toggleClass('fa-pencil');
            // Switches the forms
            $('.form').animate({
                height: "toggle",
                'padding-top': 'toggle',
                'padding-bottom': 'toggle',
                opacity: "toggle"
            }, "slow");
        });
    </script>
    <!-- //bootstrap-pop-up -->
    <!-- nav -->
    @RenderPage("MenuMain.cshtml")
    <!-- //nav -->
    <!-- banner -->
    @RenderSection("slide", required: false)

    @RenderBody()
    <!-- footer -->
    <div class="footer">
        <div class="container">
            <div class="w3ls_footer_grid">
                <div class="col-md-6 w3ls_footer_grid_left">
                    <div class="w3ls_footer_grid_left1">
                        <h2>Đăng Ký Nhận Thông Báo</h2>
                        <div class="w3ls_footer_grid_left1_pos">
                            <form action="#" method="post">
                                <input type="email" name="email" placeholder="Nhập Email của bạn" required="">
                                <input type="submit" value="Đăng Ký">
                            </form>
                        </div>
                    </div>
                </div>
                <div class="col-md-6 w3ls_footer_grid_right">
                    <a href="~/Admin"><h2>Penguin<span>Admin</span></h2></a>
                </div>
                <div class="clearfix"> </div>
            </div>
            <div class="col-md-5 w3ls_footer_grid1_left">
                <p>&copy; 2023 Penguin Movie </p>
            </div>
            <div class="clearfix"> </div>
        </div>
    </div>
    <!-- //footer -->
    <!-- Bootstrap Core JavaScript -->
    <script src="~/assets/web/js/bootstrap.min.js"></script>

</body>
</html>
